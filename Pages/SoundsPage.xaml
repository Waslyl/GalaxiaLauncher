<Page x:Class="UIKitTutorials.Pages.SoundsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UIKitTutorials.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="1165"
      Title="SoundsPage">
    <Border>
        <Grid Background="#FF2C2C2C">
            <Image Source="\Pages\fortnite.png"/>
            <Button Style="{StaticResource IconButtonsStyle}"  HorizontalAlignment="Left" Margin="32,65,0,0" VerticalAlignment="Top" Height="293" Width="251" Click="Button_Click">
                <Button.Background>
                    <ImageBrush ImageSource="/Pages/s10.jpg" Stretch="UniformToFill"/>
                </Button.Background>
                <Button Content="Button"/>
            </Button>
            <Label Content="Emplacement de votre version" FontFamily="Montserrat" FontWeight="Medium" HorizontalAlignment="Left" Margin="32,31,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>

        </Grid>
    </Border>
</Page>
    
